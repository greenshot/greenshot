<Page x:Class="Greenshot.Addon.Confluence.Forms.ConfluencePagePicker"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Loaded="Page_Loaded">
	<ListView Name="PageListView" SelectionMode="Single" ItemsSource="{Binding}"
			SelectionChanged="PageListView_SelectionChanged" Width="480" Height="500">
		<ListView.ItemTemplate>
			<DataTemplate>
				<Label Content="{Binding Title}" />
			</DataTemplate>
		</ListView.ItemTemplate>
	</ListView>
</Page>