---
permalink: update-feed.json
---
{% assign sorted-releases = site.github.releases | where: "draft", "false" | where: "prerelease", "false" %}{
	"release": {% for release in sorted-releases limit:1 %}{% for asset in release.assets %}{% if asset.name contains 'Greenshot-INSTALLER' and asset.name contains 'RELEASE' %}"{{release.tag_name}},"{% endif %}{% endfor %}{% endfor %}
	{% assign sorted-unstables = site.github.releases | where: "draft", "false" | where: "prerelease", "true" %}
	"beta": {% for release in sorted-unstables limit:1 %}{% for asset in release.assets %}{% if asset.name contains 'Greenshot-INSTALLER' and asset.name contains 'UNSTABLE' %}"{{release.tag_name}}"{% endif %}{% endfor %}{% endfor %}
}